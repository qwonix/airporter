<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import org.controlsfx.control.SearchableComboBox?>
<BorderPane xmlns="http://javafx.com/javafx/18"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.qwonix.suai.airporter.controller.searching.TicketTypeSearchController">

    <center>
        <ListView fx:id="ticketTypeListView" BorderPane.alignment="CENTER"/>
    </center>
    <top>
        <HBox BorderPane.alignment="CENTER">
            <SearchableComboBox fx:id="departureSearchCB"/>
            <SearchableComboBox fx:id="arrivalSearchCB"/>
        </HBox>
    </top>


</BorderPane>
